{
  "name": "doc-detective-global-repo",
  "version": "1.0.0",
  "description": "All the Doc Detective repos in one place.",
  "homepage": "https://github.com/doc-detective/.github#readme",
  "bugs": {
    "url": "https://github.com/doc-detective/.github/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doc-detective/.github.git"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Manny Silva <hawkeyexl@gmail.com>",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "subtree:add:common": "git subtree add --prefix=common https://github.com/doc-detective/common main --squash",
    "subtree:add:core": "git subtree add --prefix=core https://github.com/doc-detective/core main --squash",
    "subtree:add:resolver": "git subtree add --prefix=resolver https://github.com/doc-detective/resolver main --squash",
    "subtree:add:vscode": "git subtree add --prefix=vscode https://github.com/doc-detective/vscode main --squash",
    "subtree:pull:common": "git subtree pull --prefix=common https://github.com/doc-detective/common main --squash",
    "subtree:pull:core": "git subtree pull --prefix=core https://github.com/doc-detective/core main --squash",
    "subtree:pull:resolver": "git subtree pull --prefix=resolver https://github.com/doc-detective/resolver main --squash",
    "subtree:pull:vscode": "git subtree pull --prefix=vscode https://github.com/doc-detective/vscode main --squash",
    "subtree:push:common": "git subtree push --prefix=common https://github.com/doc-detective/common main",
    "subtree:push:core": "git subtree push --prefix=core https://github.com/doc-detective/core main",
    "subtree:push:resolver": "git subtree push --prefix=resolver https://github.com/doc-detective/resolver main",
    "subtree:push:vscode": "git subtree push --prefix=vscode https://github.com/doc-detective/vscode main",
    "subtree:pull:all": "npm run subtree:pull:common && npm run subtree:pull:core && npm run subtree:pull:resolver && npm run subtree:pull:vscode",
    "subtree:push:all": "npm run subtree:push:common && npm run subtree:push:core && npm run subtree:push:resolver && npm run subtree:push:vscode",
    "subtree": "node scripts/subtree-manager.js",
    "subtree:list": "node scripts/subtree-manager.js list",
    "subtree:status": "node scripts/subtree-manager.js status"
  }
}
